# 🌐 Bolt API Config
VITE_BOLT_API_URL=https://<your-bolt-url>/api
VITE_BOLT_API_KEY=your-bolt-api-key

# 🛑 Supabase (optioneel)
VITE_SUPABASE_URL=https://<your-supabase-url>.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key

# 🔄 Fallback logica (true/false)
VITE_USE_BOLT=true
VITE_USE_SUPABASE=false
VITE_USE_LOCAL_FALLBACK=true

# 🛒 Scraper instellingen
SCRAPER_MAX_PRODUCTS_PER_CATEGORY=15
SCRAPER_MAX_TOTAL_PRODUCTS=200
SCRAPER_DELAY_BETWEEN_REQUESTS=500

# 📊 Google Analytics (optioneel)
VITE_GTAG_ID=G-XXXXXXXXXX

# 🔧 Overig
VITE_ENVIRONMENT=development
VITE_BRAND_NAME=FitFi
