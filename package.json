{
  "name": "fitfi-site",
  "version": "1.0.0",
  "description": "AI-powered personal style recommendations platform",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:functions": "tsc -p functions/tsconfig.json",
    "lint": "eslint .",
    "preview": "vite preview",
    "link-check": "node scripts/link-check.js",
    "image-check": "node scripts/image-check.js",
    "test:links": "npm run link-check",
    "test:images": "npm run image-check",
    "test:all": "npm run lint && npm run link-check && npm run image-check && npm run build",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "lhci": "lhci autorun",
    "lighthouse": "npm run lhci",
    "convert-products": "node --loader ts-node/esm src/scripts/convertZalandoToBolt.ts",
    "scrape-zalando": "node --loader ts-node/esm src/scripts/scrapeZalando.ts",
    "validate-products": "node --loader ts-node/esm src/scripts/validateBoltProducts.ts",
    "upload-products": "node --loader ts-node/esm src/scripts/uploadBoltProductsToSupabase.ts",
    "scrape-and-convert": "npm run scrape-zalando && npm run validate-products"
  },
  "dependencies": {
    "@sentry/react": "^9.39.0",
    "@supabase/supabase-js": "^2.50.2",
    "cheerio": "^1.1.0",
    "dotenv": "^16.6.1",
    "framer-motion": "^10.16.16",
    "lucide-react": "^0.344.0",
    "puppeteer": "^22.15.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.22.3",
    "swr": "^2.2.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@lhci/cli": "^0.12.0",
    "@netlify/functions": "^4.1.11",
    "@playwright/test": "^1.40.0",
    "@types/node": "^20.19.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "esbuild": "^0.19.0",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
