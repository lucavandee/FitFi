{
  "name": "fitfi",
  "private": true,
  "version": "1.7.0",
  "type": "module",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "dev:netlify": "netlify dev",
    "preinstall": "node scripts/bolt-install.mjs",
    "typecheck": "tsc --noEmit --pretty",
    "build:strict": "npm run typecheck && vite build",
    "test": "vitest",
    "test:routes": "node scripts/test-routes.js",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:smoke": "playwright test tests/visual/nova-sse-health.spec.ts --headed",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "prepare": "echo 'husky skipped (Bolt)'",
    "doctor": "node scripts/doctor.mjs",
    "predev": "npm run doctor",
    "prebuild": "npm run doctor",
    "start": "vite"
  },
  "dependencies": {
    "@headlessui/react": "^2.1.2",
    "@supabase/supabase-js": "^2.39.3",
    "@tanstack/react-query": "^5.28.4",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.4",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.22.3",
    "vite": "^5.4.0",
    "@vitejs/plugin-react": "^4.3.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.css": [
      "stylelint --fix",
      "git add"
    ]
  },
  "packageManager": "npm@10",
  "devDependencies": {
    "vite": "^5.4.0",
    "@vitejs/plugin-react": "^4.3.0",
    "typescript": "^5.4.0",
    "tailwindcss": "^3.4.0",
    "postcss": "^8.4.0",
    "autoprefixer": "^10.4.0"
  }
}