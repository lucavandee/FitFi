{
  "name": "fitfi-site",
  "version": "1.5.4",
  "description": "AI-powered personal style recommendations platform",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npx playwright test",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "format": "prettier --write 'src/**/*.{ts,tsx,css,md,json}'",
    "test:types": "tsc --noEmit",
    "build:check": "npm run build",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist",
    "build:size": "npm run build && du -sh dist/*",
    "perf:audit": "lighthouse http://localhost:3000 --output=html --output-path=./lighthouse-report.html"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.6",
    "@supabase/supabase-js": "^2.53.0",
    "lucide-react": "^0.532.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-hot-toast": "^2.5.2",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "postcss": "^8.4.35",
    "react-swipeable": "^7.0.2",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^3.4.1",
    "terser": "^5.43.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "vite": "^5.4.2",
    "vite-imagetools": "^7.1.0"
  }
}
