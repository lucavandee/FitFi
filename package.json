{
  "name": "fitfi",
  "private": true,
  "version": "1.7.0",
  "type": "module",
  "packageManager": "pnpm@9.12.0",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "dev:netlify": "netlify dev",
    "preinstall": "node scripts/bolt-install.mjs",
    "typecheck": "tsc --noEmit --pretty",
    "build:strict": "npm run typecheck && vite build",
    "test": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:smoke": "playwright test tests/visual/nova-sse-health.spec.ts --headed",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "prepare": "echo 'husky skipped (Bolt)'",
    "doctor": "node scripts/doctor.mjs",
    "predev": "npm run doctor",
    "prebuild": "npm run doctor",
    "start": "vite"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.4",
    "@tanstack/react-query": "^5.90.2",
    "lucide-react": "^0.441.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^1.3.0",
    "react-hot-toast": "^2.6.0",
    "react-router-dom": "^6.26.2",
    "tailwind-merge": "^2"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.9.2",
    "vite": "^5.4.20"
  }
}
